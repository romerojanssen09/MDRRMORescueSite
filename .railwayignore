# Railway Ignore - Files to exclude from deployment

# Dependencies (will be installed during build)
/node_modules
/vendor

# Development files
/tests
/.phpunit.cache
/phpunit.xml

# IDE files
/.idea
/.vscode
/.fleet
/.nova
/.zed
/.phpactor.json

# OS files
.DS_Store
Thumbs.db

# Git
/.git
/.github

# Environment files (set in Railway dashboard)
.env
.env.backup
.env.production

# Build artifacts (will be generated)
/public/hot
/public/storage
/storage/*.key

# Logs
/storage/logs/*.log

# Cache files (but keep directory structure)
/bootstrap/cache/*.php
!/bootstrap/cache/.gitignore
/storage/framework/cache/*
!/storage/framework/cache/.gitignore
/storage/framework/sessions/*
!/storage/framework/sessions/.gitignore
/storage/framework/views/*
!/storage/framework/views/.gitignore

# Documentation (not needed in production)
/DEPLOYMENT_CHECKLIST.md
/CLEANUP_SUMMARY.md
/RAILWAY_DEPLOYMENT.md
/QUICK_START.md
/README.md
/DEPLOYMENT.md
